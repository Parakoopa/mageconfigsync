{
    "name": "punkstar/mageconfigsync",
    "description": "A utility to aid the storing of Magento configuration in version control.",
    "license": "MIT",
    "keywords": ["magento"],

    "authors": [{
        "name": "Nick Jones",
        "email": "nick@nicksays.co.uk",
        "homepage": "http://www.nicksays.co.uk",
        "role": "Developer"
    }],

    "require": {
        "php": ">=5.6.0",
        "symfony/console": "^3.3",
        "symfony/yaml": "^3.3",
        "meanbee/libmageconf": "^1.0",
        "roave/security-advisories": "dev-master",
        "monolog/monolog": "^1.22",
        "php-di/php-di": "^5.4",
        "symfony/monolog-bundle": "^3.1",
        "aura/sql": "^2.5"
    },

    "require-dev": {
        "php-vfs/php-vfs": "^1.3",
        "phpunit/phpunit": "^5.7"
    },

    "autoload": {
        "psr-0": {
            "MageConfigSync\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-0": {
            "MageConfigSyncTest\\": "tests/"
        }
    },

    "bin": ["bin/mageconfigsync"]
}
